						"header": [],
						"url": {
							"raw": "{{base_url}}/resources/export",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"resources",
								"export"
							]
						}
					},
					"response": []
				},
				{
					"name": "Export CSV with Filters",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/resources/export?name=solar&minPrice=400",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"resources",
								"export"
							],
							"query": [
								{
									"key": "name",
									"value": "solar"
								},
								{
									"key": "minPrice",
									"value": "400"
								}
							]
						}
					},
					"response": []
				}
			]
