	},
	"item": [
		{
			"name": "Resources",
			"item": [
				{
					"name": "List All Resources",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/resources/list",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"resources",
								"list"
							]
						}
					},
					"response": []
				},
				{
					"name": "Search Resources",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/resources/search?name=solar&minPrice=400",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"resources",
								"search"
							],
							"query": [
								{
									"key": "name",
									"value": "solar"
								},
								{
									"key": "minPrice",
									"value": "400"
								}
							]
						}
					},
					"response": []
				},
				{
					"name": "Import CSV",
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "formdata",
							"formdata": [
								{
									"key": "file",
									"type": "file",
									"src": "/test/sample-valid.csv"
								}
							]
						},
						"url": {
							"raw": "{{base_url}}/resources/import",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"resources",
								"import"
							]
						}
					},
					"response": []
				},
				{
					"name": "Import Invalid CSV",
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "formdata",
							"formdata": [
								{
									"key": "file",
									"type": "file",
									"src": "/test/sample-invalid.csv"
								}
							]
						},
						"url": {
							"raw": "{{base_url}}/resources/import",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"resources",
								"import"
							]
						}
					},
					"response": []
				},
				{
					"name": "Export CSV",
